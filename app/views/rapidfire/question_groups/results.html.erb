<div class="container">
  <h1><%= @question_group.name %></h1>
  
  <html>
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
    <script type="text/javascript">
    </script>
  </html>
    
  <ul class="nav nav-tabs">
    <li class="active">
      <%= link_to "JSON Results", "#json-results", "data-toggle" => "tab" %>
    </li>
    <li>
      <%= link_to "HTML Results", "#html-results", "data-toggle" => "tab" %>
    </li>
  </ul>
  <div class="tab-content">
    <div class="tab-pane active" id="json-results">
      <%= render partial: "json_results", locals: { question_group: @question_group } %>
    </div>
    <div class="tab-pane" id="html-results">
     <%= render partial: "html_results", locals: { results: @question_group_results } %>
      
    </div>
  </div>
  
  <a href="/" class="btn btn-primary" role="button" style="float: right;">뒤로 가기</a>
</div>